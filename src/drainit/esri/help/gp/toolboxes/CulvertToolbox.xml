<Toolbox Name='CulvertToolbox' Alias='CulvertToolbox'>
    <DisplayName>Culvert Toolbox</DisplayName>
    <ArrayOfGPToolInfo xsi:type='typens:ArrayOfGPToolInfo'
        xmlns:xsi='http://www.w3.org/2001/XMLSchema-instance'
        xmlns:xs='http://www.w3.org/2001/XMLSchema'
        xmlns:typens='http://www.esri.com/schemas/ArcGIS/2.9.0'>
        <GPToolInfo xsi:type='typens:GPToolInfo'>
            <Name>CulvertCapacityPytTool</Name>
            <DisplayName>NAACC Culvert Capacity</DisplayName>
            <Category></Category>
            <Help></Help>
            <Description>Measure the capacity of culverts by calculating peak flow over a hydrologically corrected digital elevation model. Culvert location data must be NAACC schema-compliant.
            </Description>
            <ProductCode>40</ProductCode>
            <ParameterInfo xsi:type='typens:ArrayOfGPParameterInfo'>
                <GPParameterInfo xsi:type='typens:GPParameterInfo'>
                    <Name>points_filepath</Name>
                    <DisplayName>Culvert Points</DisplayName>
                    <Category>Culverts</Category>
                    <DataType>GPFeatureLayer</DataType>
                    <Direction>esriGPParameterDirectionInput</Direction>
                    <ParamType>esriGPParameterTypeRequired</ParamType>
                </GPParameterInfo>
                <GPParameterInfo xsi:type='typens:GPParameterInfo'>
                    <Name>raster_flowdir_filepath</Name>
                    <DisplayName>Flow Direction</DisplayName>
                    <Category>DEM</Category>
                    <DataType>GPRasterLayer</DataType>
                    <Direction>esriGPParameterDirectionInput</Direction>
                    <ParamType>esriGPParameterTypeRequired</ParamType>
                </GPParameterInfo>
                <GPParameterInfo xsi:type='typens:GPParameterInfo'>
                    <Name>raster_flowlen_filepath</Name>
                    <DisplayName>Flow Length</DisplayName>
                    <Category>DEM</Category>
                    <DataType>GPRasterLayer</DataType>
                    <Direction>esriGPParameterDirectionInput</Direction>
                    <ParamType>esriGPParameterTypeRequired</ParamType>
                </GPParameterInfo>
                <GPParameterInfo xsi:type='typens:GPParameterInfo'>
                    <Name>raster_slope_filepath</Name>
                    <DisplayName>Slope</DisplayName>
                    <Category>DEM</Category>
                    <DataType>GPRasterLayer</DataType>
                    <Direction>esriGPParameterDirectionInput</Direction>
                    <ParamType>esriGPParameterTypeRequired</ParamType>
                </GPParameterInfo>
                <GPParameterInfo xsi:type='typens:GPParameterInfo'>
                    <Name>raster_curvenumber_filepath</Name>
                    <DisplayName>Curve Number</DisplayName>
                    <Category>Curve Number</Category>
                    <DataType>GPRasterLayer</DataType>
                    <Direction>esriGPParameterDirectionInput</Direction>
                    <ParamType>esriGPParameterTypeRequired</ParamType>
                </GPParameterInfo>
                <GPParameterInfo xsi:type='typens:GPParameterInfo'>
                    <Name>precip_src_config_filepath</Name>
                    <DisplayName>Precipitation Configuration File</DisplayName>
                    <Category>Rainfall</Category>
                    <DataType>DEFile</DataType>
                    <Direction>esriGPParameterDirectionInput</Direction>
                    <ParamType>esriGPParameterTypeRequired</ParamType>
                </GPParameterInfo>
                <GPParameterInfo xsi:type='typens:GPParameterInfo'>
                    <Name>output_points_filepath</Name>
                    <DisplayName>Result Points</DisplayName>
                    <Category>Outputs</Category>
                    <DataType>DEFeatureClass</DataType>
                    <Direction>esriGPParameterDirectionOutput</Direction>
                    <ParamType>esriGPParameterTypeRequired</ParamType>
                </GPParameterInfo>
                <GPParameterInfo xsi:type='typens:GPParameterInfo'>
                    <Name>output_sheds_filepath</Name>
                    <DisplayName>Result Delineations</DisplayName>
                    <Category>Outputs</Category>
                    <DataType>DEFeatureClass</DataType>
                    <Direction>esriGPParameterDirectionOutput</Direction>
                    <ParamType>esriGPParameterTypeDerived</ParamType>
                </GPParameterInfo>
            </ParameterInfo>
        </GPToolInfo>
        <GPToolInfo xsi:type='typens:GPToolInfo'>
            <Name>NaaccEtlPytTool</Name>
            <DisplayName>NAACC Table Ingest</DisplayName>
            <Category></Category>
            <Help></Help>
            <Description>Read in, validate, and extend a NAACC-compliant source table, saving the output as geodata (e.g., a file geodatabase feature class) for use in other culvert analysis tools.
            </Description>
            <ProductCode>40</ProductCode>
            <ParameterInfo xsi:type='typens:ArrayOfGPParameterInfo'>
                <GPParameterInfo xsi:type='typens:GPParameterInfo'>
                    <Name>naacc_src_table</Name>
                    <DisplayName>NAACC CSV</DisplayName>
                    <Category></Category>
                    <DataType>DEFile</DataType>
                    <Direction>esriGPParameterDirectionInput</Direction>
                    <ParamType>esriGPParameterTypeRequired</ParamType>
                </GPParameterInfo>
                <GPParameterInfo xsi:type='typens:GPParameterInfo'>
                    <Name>output_folder</Name>
                    <DisplayName>Output Folder</DisplayName>
                    <Category></Category>
                    <DataType>DEFolder</DataType>
                    <Direction>esriGPParameterDirectionOutput</Direction>
                    <ParamType>esriGPParameterTypeRequired</ParamType>
                </GPParameterInfo>
                <GPParameterInfo xsi:type='typens:GPParameterInfo'>
                    <Name>output_fc</Name>
                    <DisplayName>Output Feature Class</DisplayName>
                    <Category></Category>
                    <DataType>DEFeatureClass</DataType>
                    <Direction>esriGPParameterDirectionOutput</Direction>
                    <ParamType>esriGPParameterTypeRequired</ParamType>
                </GPParameterInfo>
            </ParameterInfo>
        </GPToolInfo>
        <GPToolInfo xsi:type='typens:GPToolInfo'>
            <Name>NoaaRainfallEtlPytTool</Name>
            <DisplayName>NOAA Rainfall Raster Data Downloader</DisplayName>
            <Category></Category>
            <Help></Help>
            <Description>Download rainfall rasters for your study area from NOAA. By default, this tool acquires rainfall data for 24hr events for frequencies from 1 to 1000 year. All rasters are saved to the user-specified folder. A JSON file is automatically created; this file is used as a required input to other tools. Note that NOAA Atlas 14 precip values are in millimeters.
            </Description>
            <ProductCode>40</ProductCode>
            <ParameterInfo xsi:type='typens:ArrayOfGPParameterInfo'>
                <GPParameterInfo xsi:type='typens:GPParameterInfo'>
                    <Name>aoi_geo</Name>
                    <DisplayName>Area of Interest</DisplayName>
                    <Category></Category>
                    <DataType>GPFeatureLayer</DataType>
                    <Direction>esriGPParameterDirectionInput</Direction>
                    <ParamType>esriGPParameterTypeRequired</ParamType>
                </GPParameterInfo>
                <GPParameterInfo xsi:type='typens:GPParameterInfo'>
                    <Name>target_raster</Name>
                    <DisplayName>Reference Raster</DisplayName>
                    <Category></Category>
                    <DataType>GPRasterLayer</DataType>
                    <Direction>esriGPParameterDirectionInput</Direction>
                    <ParamType>esriGPParameterTypeOptional</ParamType>
                </GPParameterInfo>
                <GPParameterInfo xsi:type='typens:GPParameterInfo'>
                    <Name>out_folder</Name>
                    <DisplayName>Output Folder</DisplayName>
                    <Category></Category>
                    <DataType>DEFolder</DataType>
                    <Direction>esriGPParameterDirectionInput</Direction>
                    <ParamType>esriGPParameterTypeRequired</ParamType>
                </GPParameterInfo>
                <GPParameterInfo xsi:type='typens:GPParameterInfo'>
                    <Name>out_file_name</Name>
                    <DisplayName>Output Rainfall Configuration File Name</DisplayName>
                    <Category></Category>
                    <DataType>GPString</DataType>
                    <Direction>esriGPParameterDirectionOutput</Direction>
                    <ParamType>esriGPParameterTypeRequired</ParamType>
                </GPParameterInfo>
            </ParameterInfo>
        </GPToolInfo>
    </ArrayOfGPToolInfo>
</Toolbox>